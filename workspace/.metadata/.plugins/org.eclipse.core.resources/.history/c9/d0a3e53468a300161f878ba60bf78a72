		// TODO Auto-generated method stub
		
		switch (color) {
		case "ROJO":
			return new Red;
			break;

		default:
			break;
		}
		return null;
	}

	@Override
	public Shape getShape(String shape) {
		// TODO Auto-generated method stub
		return null;
	}


}
