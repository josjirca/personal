
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Cliente c = new Cliente();
		
		new Thread()
		{
			@Override
			public void run(){
				c.retiro(1000);
			}
		}.start();
		
		new Thread()
		{
			@Override
			public void run(){
				c.deposito(2000);
			}
		}.start();
		
		new Thread()
		{
			@Override
			public void run(){
				c.retiro(1200);
			}
		}.start();
		
		
		new Thread()
		{
			@Override
			public void run(){
				c.deposito(200);
			}
		}.start();
	}

}
