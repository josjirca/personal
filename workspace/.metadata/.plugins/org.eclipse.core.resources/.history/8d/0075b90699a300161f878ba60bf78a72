
public class Hilo extends Thread{

	private int tipo;
	
	public void setTipo(int tipo) {
		this.tipo = tipo;
	}
	
	public void run()
	{
		if (tipo == 1)
		{
			StringBuilder s = null;
			for (int i = 0; i <10000; i++) {
				s.append("A");
				System.out.println(s);			
			}
		} else
		{
			for (int i = 0; i <10000; i++) {				
				System.out.println(i);			
			}
		}
		
				
	}
}
